{
  "common_variable": {
    "driver_name": "Telescent",
    "driver_module": ["telescent.telescent_driver_handler", "TelescentDriverHandler"],
    "connection_type": "ssh",
    "device_prompt": "[0-9a-zA-Z@][0-9a-zA-Z@]*>",
    "server_timeout": 10,
    "runtime_configuration": "telescent_runtime_configuration.json"
  },

  "cli_variable": {
    "ssh": ["common.cli.ssh_session", "SSHSession"],
    "telnet": ["common.cli.telnet_session", "TelnetSession"],
    "tcp": ["common.cli.tcp_session", "TCPSession"]
  },

  "driver_variable": {
    "service_mode": "tl1",
    "port_mode": "logical",

    "resource_name": [
      "Chassis {0}",
      "Port {0}"
    ],
    "resource_family_name": [
      "L1 Connectivity Switch",
      "L1 Switch Port"
    ],
    "resource_model_name": [
      "{0}",
      "Port {0}"
    ]
  }
}